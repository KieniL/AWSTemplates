        "JiraServer": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "ImageId": "ami-0cc0a36f626a4fdf5",
                "InstanceType": {
                    "Ref": "InstanceTypeParameter"
                },
                "IamInstanceProfile": {
                    "Ref": "SSMEC2Profile"
                },
                "UserData": {
                    "Fn::Base64": {
                        "Fn::Join": [
                            "\n",
                            [
                                "#!/bin/sh",
                                "#install java and postgres",
                                "sudo wget https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk-11.0.5%2B10/OpenJDK11U-jdk_x64_linux_hotspot_11.0.5_10.tar.gz",
                                "sudo tar -xf OpenJDK11U-jdk_x64_linux_hotspot_11.0.5_10.tar.gz -C /home/ubuntu",
                                "sudo sed -i -e '1iJAVA_HOME=/home/ubuntu/jdk-11.0.5+10' /etc/environment",			
                                "sudo sed -i 's/PATH=\"/PATH=\"$JAVA_HOME/bin:/g' /etc/environment",
                                "source /etc/environment",
                                "sudo apt-get update && sudo apt-get install -y postgresql postgresql-contrib",
                                "#Get Variables",
                                "user=jiradbuser",
                                "pass=jiradb",
                                "jirauser=jirauser",
                                "jirauserpw=jira",
                                "postgrespw=postgres",
                                "databasename=jiradb",
                                "#Create user",
                                "sudo  useradd $user",
                                "echo \"$user:$pass\" | sudo chpasswd -e",
                                "#Create user",
                                "sudo  useradd $jirauser",
                                "echo \"$jirauser:$jirauserpw\" | sudo chpasswd -e",
                                "#get PostgresVersion",
                                "pgversion=$(psql -V | cut -d\")\" -f2 | cut -d\"(\" -f1 | sed -e 's/^[[:space:]]*//' | cut -d\".\" -f1)",
                                "#Restart Postgresl",
                                "sudo service postgresql restart",
                                "#Alter DB user and Create Database",
                                "sudo -u postgres psql -U postgres -c \"ALTER USER postgres with password '$postgrespw'\";",
                                "sudo -u postgres psql -U postgres -c \"CREATE USER $user with PASSWORD '$pass';\"",
                                "sudo -u postgres psql -U postgres -c \"CREATE DATABASE $databasename WITH ENCODING 'UTF8'\";",
                                "sudo -u postgres psql -U postgres -c \"ALTER DATABASE $databasename OWNER to $user\";",
                                "sudo service postgresql restart",
                                "#Download JIRA 8.6",
                                "sudo wget -O atlassian-jira-software-8.6.0.tar.gz https://www.atlassian.com/software/jira/downloads/binary/atlassian-jira-software-8.6.0.tar.gz",
                                "#Create Installation directory",
                                "sudo mkdir /jirasoftwareinstall",
                                "#unzip Archive file",
                                "sudo tar -xzf atlassian-jira-software-8.6.0.tar.gz -C /jirasoftwareinstall",
                                "#Create Homedirectory",
                                "sudo mkdir /jirahome",
                                "#set jirauser as owner for install and home directory",
                                "sudo chown -R $jirauser /jirasoftwareinstall",
                                "sudo chown -R $jirauser /jirahome",
                                "sudo chmod -R u=rwx,go-rwx /jirasoftwareinstall",
                                "sudo chmod -R u=rwx,go-rwx /jirahome",
								"sudo sed -i -e '1iJIRA_HOME=\/jirahome' /etc/environment",
								"source /etc/environment",
                                "sudo /jirasoftwareinstall/atlassian-jira-software-8.6.0-standalone/bin/start-jira.sh",
                                ""
                            ]
                        ]
                    }
